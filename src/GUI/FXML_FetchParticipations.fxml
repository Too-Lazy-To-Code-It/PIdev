<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="900.0" styleClass="backcolor" stylesheets="@../css/menustyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.FXML_FetchParticipationsController">
   <children>
      <ScrollPane layoutX="156.0" layoutY="108.0" prefHeight="492.0" prefWidth="745.0" styleClass="backcolor" stylesheets="@../css/menustyle.css">
         <content>
            <GridPane fx:id="Participation_Grid" alignment="TOP_CENTER" hgap="30.0" prefHeight="477.0" prefWidth="745.0" styleClass="backcolor" stylesheets="@../css/menustyle.css" vgap="30.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
               <padding>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
               </padding>
            </GridPane>
         </content>
      </ScrollPane>
      <Label fx:id="challenge_desc" alignment="TOP_LEFT" layoutX="159.0" layoutY="8.0" prefHeight="86.0" prefWidth="739.0" text="Description" textFill="WHITE">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Label>
      <ImageView fx:id="challenge_img" fitHeight="86.0" fitWidth="138.0" layoutX="15.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true" />
      <VBox fx:id="vbox" alignment="TOP_CENTER" layoutX="7.0" layoutY="165.0" prefHeight="319.0" prefWidth="144.0" spacing="20.0" styleClass="backcolor" stylesheets="@../css/menustyle.css">
         <children>
            <Button fx:id="imported_img" mnemonicParsing="false" onAction="#importImage" prefHeight="25.0" prefWidth="146.0" style="-fx-background-color: linear-gradient(from 25px 25px to 100px 70px, #c10c99,  #071330); -fx-cursor: hand;" stylesheets="@../css/blogstyle.css" text="Select Image" textFill="WHITE" />
            <TextArea fx:id="new_desc" prefHeight="252.0" prefWidth="138.0" />
            <Button fx:id="participate_id" mnemonicParsing="false" onAction="#participate" prefHeight="30.0" prefWidth="104.0" style="-fx-background-color: linear-gradient(from 25px 25px to 100px 70px, #c10c99,  #071330); -fx-cursor: hand;" stylesheets="@../css/blogstyle.css" text="Participate" textFill="WHITE" />
            <Button mnemonicParsing="false" onAction="#goMenu" prefHeight="30.0" prefWidth="104.0" style="-fx-background-color: linear-gradient(from 25px 25px to 100px 70px, #c10c99,  #071330); -fx-cursor: hand;" stylesheets="@../css/blogstyle.css" text="return" textFill="WHITE" />
         </children>
      </VBox>
   </children>
</AnchorPane>
