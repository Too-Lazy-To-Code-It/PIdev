<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@Swag.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.FXML_SupprimerProduitController">
   <children>
      <Label layoutX="35.0" layoutY="58.0" prefHeight="63.0" prefWidth="510.0" text="Entrer le nom du produit que vous souhaitez supprimer" textFill="WHITE">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="nomText" layoutX="174.0" layoutY="163.0" />
      <Label layoutX="114.0" layoutY="168.0" prefHeight="17.0" prefWidth="60.0" text="Nom:" textFill="#eea4a4">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="supp" layoutX="348.0" layoutY="158.0" mnemonicParsing="false" onAction="#supprimer" prefHeight="36.0" prefWidth="96.0" text="Supprimer" />
      <Button fx:id="retour" layoutX="468.0" layoutY="18.0" mnemonicParsing="false" onAction="#GotoAccueil" prefHeight="27.0" prefWidth="86.0" text="Retour" />
   </children>
</AnchorPane>
