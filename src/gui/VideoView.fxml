<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane id="AnchorPane" prefHeight="566.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.VideoViewController">
   <children>
      <MediaView fx:id="mediaView" fitHeight="480.0" fitWidth="720.0" layoutY="52.0" />
      <Button fx:id="playButton" layoutX="103.0" layoutY="532.0" mnemonicParsing="false" onAction="#playMedia" text="Play" />
      <Button fx:id="pauseButton" layoutX="141.0" layoutY="532.0" mnemonicParsing="false" onAction="#pauseMedia" text="Pause" />
      <Button fx:id="resetButton" layoutX="188.0" layoutY="532.0" mnemonicParsing="false" onAction="#resetMedia" text="Reset" />
      <ImageView fitHeight="40.0" fitWidth="38.0" layoutX="668.0" layoutY="14.0" onMouseClicked="#returne" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/return.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
